{
  "name": "webpayby/webpayby-payment-json-sdk",
  "description": "SDK for simple making payment request to Webpay",
  "type": "library",
  "version": "1.0.1",
  "require": {
    "php": ">=7.1",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Webpayby\\Payment\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
